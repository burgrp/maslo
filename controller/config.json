{
    "machine": {
        "module": "./machine.js",
        "drivers": {
            "virtual": {
                "module": "./driver-virtual.js",
                "stopPositions": {
                    "z": [
                        0,
                        5000
                    ]
                }
            },
            "i2c": {
                "module": "./driver-i2c.js",
                "bus": "-> $.I2C",
                "motorAddresses": {
                    "a": 80,
                    "b": 81,
                    "z": 82
                }
            }
        },
        "driver": "-> $.MACHINE_DRIVER || 'virtual'",
        "machineCheckIntervalMs": 100,
        "moveSpeedRapidMmPerMin": 1500,
        "moveSpeedCuttingMmPerMin": 200,
        "motorsShaftDistanceMm": 3500,
        "workspaceWidthMm": 2500,
        "workspaceHeightMm": 1250,
        "motorsToWorkspaceVerticalMm": 500,
        "motorDampingTrigger": 0.8,
        "motorDampingStep": 0.1,
        "motorMinDuty": 0.2,
        "minDepartureSpeedMmPerMin": 400,
        "maxDepartureSpeedMmPerMinPerMm": 200,
        "minArrivalSpeedMmPerMin": 200,
        "maxArrivalSpeedMmPerMinPerMm": 400,
        "targetToleranceMm": 1,
        "manualMoveMm": {
            "xy": 100,
            "ab": 100,
            "z": 1
        },
        "motors": {
            "a": {
                "maxRpm": 7800,
                "encoderPpr": 17,
                "gearRatio": 280,
                "mmPerRev": 60
            },
            "b": {
                "maxRpm": 7800,
                "encoderPpr": 17,
                "gearRatio": 280,
                "mmPerRev": 60
            },
            "z": {
                "maxRpm": 9900,
                "encoderPpr": 11,
                "gearRatio": 90,
                "mmPerRev": 1
            }
        },
        "relays": {
            "spindle": {}
        }
    },
    "webserver": {
        "module": "@device.farm/webglue",
        "modules": [
            {
                "module": "./ui.js",
                "machine": "-> machine"
            }
        ]
    }
}